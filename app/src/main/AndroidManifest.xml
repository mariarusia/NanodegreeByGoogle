<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicstructureapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ArtistActivity"
            android:label="@string/title_activity_artist" />
        <activity android:name=".AlbumActivity" android:label="@string/title_album_activity"/>
        <activity android:name=".GenreActivity" android:label="@string/title_genre_activityt" />
        <activity android:name=".PlayingNowActivity" android:label="@string/title_activity_now_playing"/>
        <activity android:name=".TrackActivity" android:label="@string/title_track_activity"/>
    </application>

</manifest>